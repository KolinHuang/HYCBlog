                    <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">print</span><span class="o">(</span><span class="n">maxQueue</span><span class="o">.</span><span class="na">max_value</span><span class="o">()+</span><span class="s">","</span><span class="o">);</span>
