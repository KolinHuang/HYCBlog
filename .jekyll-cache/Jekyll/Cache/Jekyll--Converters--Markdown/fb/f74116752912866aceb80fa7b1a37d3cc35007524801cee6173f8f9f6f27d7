    <span class="kt">boolean</span> <span class="nf">isCompare</span><span class="o">(</span><span class="nc">TreeNode</span> <span class="no">A</span><span class="o">,</span> <span class="nc">TreeNode</span> <span class="no">B</span><span class="o">){</span>
